{
  "name": "Make WSJ & NYT Great Again",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Get around the paywall for many WSJ, NYT & FT content",
  "homepage_url": "http://blog.jinsongli.com",
  "icons": {
    "48": "icons/eric_cartman.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
      {
        "matches": ["https://cooking.nytimes.com/*"],
        "css" : ["css/nyt.css"]
      }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*.wsj.com/*",
    "http://*.wsj.com/*",
    "https://*.wsj.net/*",
    "https://*.ft.com/*",
    "http://*.ft.com/*",
    "https://*.nytimes.com/*",
    "http://*.nytimes.com/*",
    "https://*.nyt.com/*",
    "http://*.nyt.com/*",
    "https://*.bloomberg.com/*",
    "http://*.bloomberg.com/*",
    "https://*.bwbx.io/*",
    "http://*.bwbx.io/*",
    "https://*.washingtonpost.com/*",
    "http://*.washingtonpost.com/*",
    "http://*.philly.com/*",
    "https://*.bizjournals.com/*",
    "http://*.bizjournals.com/*",
    "http://www.kleinezeitung.at/*"
   ]
}
